<p id="notice"><%= notice %></p>

<h1>Prizes</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Placing</th>
      <th>User</th>
      <th>Ticket</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prizes.each do |prize| %>
      <tr>
        <td><%= prize.description %></td>
        <td><%= prize.placing %></td>
        <td><%= prize.user_id %></td>
        <td><%= prize.ticket_id %></td>
        <td><%= link_to 'Show', prize %></td>
        <td><%= link_to 'Edit', edit_prize_path(prize) %></td>
        <td><%= link_to 'Destroy', prize, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prize', new_prize_path %>
