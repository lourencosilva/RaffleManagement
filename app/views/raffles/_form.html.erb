<%= simple_form_for raffle do |f| %>
  <%= f.input :title%>
  <%= f.input :description%>
  <%= f.input :ticket_value%>
  <%= f.input :probable_draw_at, as: :date, html5: true %>
  <%= f.input :start_sale_at, as: :date, html5: true %>
  <%= f.input :finishes_sale_at, as: :date, html5: true %>
  <%= f.input :drawn_at, as: :date, html5: true %>
  <%= f.association :user %>
  <%= f.association :kind %>
  <%= f.button :submit, class: 'btn-primary'%>
<% end %>
